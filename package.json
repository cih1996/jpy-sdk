{
  "name": "jpy",
  "version": "1.0.0",
  "description": "跨平台中间件 SDK - 支持 WebSocket 和 HTTP API 通信",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.cjs",
      "types": "./dist/middleware/index.d.ts"
    },
    "./admin-device": {
      "import": "./dist/admin-device/index.js",
      "require": "./dist/admin-device/index.cjs",
      "types": "./dist/admin-device/index.d.ts"
    },
    "./admin-dhcp": {
      "import": "./dist/admin-dhcp/index.js",
      "require": "./dist/admin-dhcp/index.cjs",
      "types": "./dist/admin-dhcp/index.d.ts"
    },
    "./device-modify": {
      "import": "./dist/device-modify/index.js",
      "require": "./dist/device-modify/index.cjs",
      "types": "./dist/device-modify/index.d.ts"
    },
    "./shared": {
      "import": "./dist/shared/index.js",
      "require": "./dist/shared/index.cjs",
      "types": "./dist/shared/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "websocket",
    "middleware",
    "sdk",
    "cross-platform",
    "typescript",
    "node",
    "browser",
    "electron",
    "bun",
    "deno",
    "vue",
    "svelte",
    "react"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "msgpackr": "^1.10.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.3.0"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/jpy-sdk.git"
  },
  "bugs": {
    "url": ""
  },
  "homepage": ""
}
