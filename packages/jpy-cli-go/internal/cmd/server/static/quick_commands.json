[
  {
    "category": "服务器初始化",
    "items": [
      {"name": "批量创建服务器中间件到分组", "cmd": "jpy middleware auth create"},
      {"name": "选择活动分组服务器", "cmd": "jpy middleware auth select default"},
      {"name": "移除连接失败的服务器（软删除）", "cmd": "jpy middleware remove"},
      {"name": "尝试登录失败的服务器", "cmd": "jpy middleware relogin"},
      {"name": "列出登录失败的服务器", "cmd": "jpy middleware list --has-fail"},
      {"name": "清空服务器地址（永久）", "cmd": "jpy middleware remove --force --all"}
    ]
  },
  {
    "category": "服务器列表筛选",
    "items": [
      {"name": "获取未授权服务器", "cmd": "jpy middleware device status --auth-failed"},
      {"name": "获取未全部上线服务器", "cmd": "jpy middleware device status --biz-online-lt 20"}
    ]
  },
  {
    "category": "服务器控制",
    "items": [
      {"name": "全自动授权服务器", "cmd": "jpy middleware admin auto-auth"},
      {"name": "全自动设置集控平台地址", "cmd": "jpy middleware admin update-cluster"},
      {"name": "查看链接中间件shell到root密码", "cmd": "jpy middleware ssh 192.168.10.201"}
    ]
  },
  {
    "category": "设备列表筛选",
    "items": [
      {"name": "获取已开启USB设备", "cmd": "jpy middleware device list --filter-adb true"},
      {"name": "获取没有IP的设备", "cmd": "jpy middleware device list --filter-has-ip false"},
      {"name": "获取没有SN的设备", "cmd": "jpy middleware device list --filter-uuid false"}
    ]
  },
  {
    "category": "设备控制",
    "items": [
      {"name": "将无SN的设备切换到USB模式", "cmd": "jpy middleware device usb --mode usb --filter-uuid false"},
      {"name": "将无SN的设备切换到OTG模式", "cmd": "jpy middleware device usb --mode host --filter-uuid false"},
      {"name": "重启所有无sn到设备", "cmd": "jpy middleware device reboot --filter-uuid false"},
      {"name": "将无IP的设备切换到USB模式", "cmd": "jpy middleware device usb --mode usb --filter-has-ip false"},
      {"name": "将无IP的设备切换到OTG模式", "cmd": "jpy middleware device usb --mode host --filter-has-ip false"},
      {"name": "重启所有无IP的设备", "cmd": "jpy middleware device reboot --filter-has-ip false"},
      {"name": "查看指定设备guard日志", "cmd": "jpy middleware device log --server \"192.168.30.203\" --seat 1"}
    ]
  },
  {
    "category": "系统命令",
    "items": [
      {"name": "帮助", "cmd": "jpy --help"},
      {"name": "清屏", "cmd": "clear"},
      {"name": "退出", "cmd": "exit"}
    ]
  }
]